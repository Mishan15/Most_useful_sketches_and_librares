void loop(void) {
  q = !q;
  if (i > 128) i = 0;
  else i += 10;
  if (i < 50 || i > 110)  draw_state = !draw_state;
  else if (i >= 50 && i < 60) draw_state = 2;
  else if (i >= 60 && i < 70) draw_state = 3;
  else if (i >= 70 && i < 80) draw_state = 4;
  else if (i >= 80 && i < 90) draw_state = 5;
  else if (i >= 90 && i < 100) draw_state = 6;
  else if (i >= 100 && i < 110) draw_state = 7;
  u8g.firstPage();
  do {
    draw();
  } while ( u8g.nextPage() );
  delay(100);
}
