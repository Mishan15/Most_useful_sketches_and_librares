void draw(void) {
  switch (draw_state) {
    case 0: u8g.drawXBMP( 60, 17, u8g_width, u8g_height, forvard); break;
    case 1: u8g.drawXBMP( 60, 17, u8g_width, u8g_height, back); break;
    case 2: u8g.drawXBMP( 60, 17, u8g_width, u8g_height, sit); break;
    case 3: u8g.drawXBMP( 60, 17, u8g_width, u8g_height, jump); break;
    case 4: u8g.drawXBMP( 60, 17, u8g_width, u8g_height, fly); break;
    case 5: u8g.drawXBMP( 45, 17, u8g_width, u8g_height, land); break;
    case 6: u8g.drawXBMP( 40, 17, u8g_width, u8g_height, deflection); break;
    case 7: u8g.drawXBMP( 45, 17, u8g_width, u8g_height, stand); break;
  }
  if (q)u8g.drawXBMP( 0, 0, 128, 15, sky1); 
  else u8g.drawXBMP( 0, 0, 128, 15, sky2);
  u8g.drawLine(0, 63, 128, 63);
  u8g.drawLine(0, 62, 128, 62);
  u8g.drawDisc(i, 53, 9);
}


